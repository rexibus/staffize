<div class ="bookings">
<h1>My Bookings</h1>

<% @bookings.each do |booking| %>
  <h2><%= booking.job_listing.title %></h2>
  <h4><%= booking.status %></h4>
  <h4>starting date: <%= booking.start_date.strftime("%d/%m/%Y") %></h4>
  <h4>ending date: <%= booking.end_date.strftime("%d/%m/%Y") %></h4>
  <%= link_to "Delete", booking_path(booking),
      method: :delete,
      data: { confirm: "Are you sure?" } %>
<% end %>
</div>



